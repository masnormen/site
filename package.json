    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "babel-plugin-react-compiler": "19.0.0-beta-714736e-20250131",
    "concurrently": "^9.2.0",
    "esbuild": "^0.24.2",
    "fast-glob": "^3.3.3",
    "gray-matter": "^4.0.3",
    "postcss": "^8.5.6",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-mdx-frontmatter": "^5.2.0",
    "shiki": "^2.5.0",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.6",
    "twoslash": "^0.2.12",
